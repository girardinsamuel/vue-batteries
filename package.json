{
  "name": "vue-batteries",
  "version": "0.1.0",
  "description": "A renderless not opiniated components and directive library to quickly build UI with Vue.js",
  "author": "Samuel Girardin",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "demo": "vue-cli-service demo",
    "docs": "npm run docs:serve",
    "docs:build": "vue-cli-service docs --mode build",
    "docs:serve": "vue-cli-service docs --mode serve",
    "prepublish": "vue-cli-service lint && vue-cli-service docs --mode build && vue-cli-service build",
    "start": "vue-cli-service serve"
  },
  "main": "dist/vue-batteries.common.js",
  "module": "dist/vue-batteries.esm.js",
  "unpkg": "dist/vue-batteries.umd.min.js",
  "files": [
    "dist/vue-batteries.common.js",
    "dist/vue-batteries.umd.min.js",
    "dist/vue-batteries.umd.js",
    "dist/vue-batteries.esm.js",
    "src"
  ],
  "dependencies": {
    "@vue/composition-api": "^0.5.0",
    "core-js": "^3.6.4",
    "lodash.merge": "^4.6.2",
    "tailwindcss": "^1.2.0",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue": "^2.6.11",
    "vue-cli-plugin-p11n": "~0.4.0",
    "vue-cli-plugin-tailwind": "~1.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "bugs": "https://github.com/girardinsamuel/vue-batteries/issues",
  "contributors": [
    "Samuel Girardin <samuelgirardin@gmail.com>"
  ],
  "jsdelivr": "dist/vue-batteries.umd.min.js",
  "repository": "https://github.com/girardinsamuel/vue-batteries.git",
  "sideeffects": false
}
